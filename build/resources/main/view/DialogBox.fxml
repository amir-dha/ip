<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root type="HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
         spacing="10.0">
    <children>
        <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
        <Label fx:id="dialog" text="Label" wrapText="true" maxWidth="250.0" minWidth="50.0"/>
    </children>
    <padding>
        <Insets top="5.0" bottom="5.0" left="10.0" right="10.0"/>
    </padding>
</fx:root>










